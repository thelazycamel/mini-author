<div class="form-group">
  <%= form.label :name %>
  <%= form.text_field :name, class: "form-control" %>
</div>
<div class="form-group" %>
  <%= form.label :site_id %>
  <%= form.select(:site_id, sites_for_select(@learning_object.site_id), {}, class: "form-control") %>
</div>
<div class="form-group">
  <%= form.label :lo %>
  <%= form.file_field :lo, class: "form-control" %>
</div>
<div class="form-group">
  <%= form.label :info_xml %>
  <%= form.file_field :info_xml, class: "form-control" %>
</div>
<%= form.submit %>
